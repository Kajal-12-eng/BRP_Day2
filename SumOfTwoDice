public class SumOfTwoDice {
    public static void main(String[] args) {
        int dice1 = (int) Math.floor(Math.random() * 10 % 6);
        int dice2 = (int) Math.floor(Math.random() * 10 % 6);
        int sum = dice1 + dice2;
        System.out.println("Sum of Two dice value is : " + sum);
    }
}
